{% block body %}
<div class="base">

    <!-- Page -->
    <div class="page">

        <!-- Page Header-->
        <section class="page-title page-title-bg fixed-bg overlay dark-5 padding-top-160 padding-bottom-80">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <h2 class="white-text">2nd assistant position</h2>
                        <span class="white-text">Ref. 1007</span>
                        <ol class="breadcrumb">
                            <li>
                                <div class="portfolio-nav">
                                    <a href="#!" class="waves-effect waves-dark"><i class="fa fa-angle-left" aria-hidden="true"></i> Prev</a>
                                    <a href="#!" class="waves-effect waves-dark"><i class="fa fa-th-large" aria-hidden="true"></i></a>
                                    <a href="#!" class="waves-effect waves-dark">Next <i class="fa fa-angle-right" aria-hidden="true"></i></a>
                                </div>
                            </li>
                        </ol>
                    </div>
                </div>
            </div>
        </section>

        <!-- Page Content-->
        <section class="single-project-section section-padding light-gray-bg">
            <div class="container">
                <div class="project-overview">
                    <div class="row mb-80">
                        <div class="col-xs-12 col-md-8">
                            <p class="ref grey-text no-margin">Ref. {{ offreEmploi.ref }}</p>
                            <h2>{{ offreEmploi.jobTitle }}</h2>
                            <p>{{ offreEmploi.notes }}</p>
                            <p>{{ offreEmploi.description }}</p>
                           
                        </div>

                        <div class="col-xs-12 col-md-4 quick-overview">
                            <ul class="portfolio-meta">
                                <li><span> Pulished at </span>{{ offreEmploi.dateCreation|date('Y-m-d') }}</li>
                                <li><span> Position </span>{{ offreEmploi.jobCategory }}</li>
                                <li><span> Contract Type </span>{{ offreEmploi.jobType }}</li>
                                <li><span> Salary </span>{{ offreEmploi.salary }} &euro;</li>
                                <li><span> Location </span>{{ offreEmploi.location }}</li>
                                <li><span> Starting date </span>{{ offreEmploi.closingDate|date('Y-m-d') }}</li>
                            </ul>
                            <a class="btn btn-block gradient primary mt-30 waves-effect waves-light" href="{{ path('postuler', {'id': offreEmploi.id}) }}">Apply for this job</a>
                            <div class="btn btn-block btn-success mt-30 waves-effect waves-light disabled">You have applied for this job</div>
                        </div>
                    </div>
                </div>

                <nav class="single-post-navigation no-margin" role="navigation">
                    <div class="row">

                        <div class="col-xs-6 col-sm-6 col-md-4">
                            <div class="previous-post-link">
                             {% if offrePrecedente is not null %}
                                <a class="btn border primary waves-effect waves-dark" href="{{ path('app_offre_show', {'id': offrePrecedente.id}) }}">
                                    <i class="fa fa-long-arrow-left"></i>Previous
                                </a>
                                {% endif %}
                            </div>
                        </div>
                        <div class="hidden-xs hidden-sm col-md-4"></div>


                        <div class="col-xs-6 col-sm-6 col-md-4">
                            <div class="next-post-link">
                            {% if offreSuivante is not null %}
                                <a class="btn border primary waves-effect waves-dark" href="{{ path('app_offre_show', {'id': offreSuivante.id}) }}">
                                    Next <i class="fa fa-long-arrow-right"></i>
                                </a>
                                 {% endif %}
                            </div>
                        </div>

                    </div>
                </nav>
            </div>
        </section>

    </div>

</div>
{% endblock %}

